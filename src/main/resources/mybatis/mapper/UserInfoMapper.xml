<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.min.cinemagreen.mapper.IUserInfoMapper">

    <select id="getUserList" resultType="UserInfoDTO">
        SELECT
            user_no,
            name,
            email,
            mobile,
            signup_dt
        FROM
            user_t
        ORDER BY
            signup_dt DESC
    </select>
  
  
</mapper>